<h1>Edit Character</h1>

<%= form_for ([@house, @character]) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :house %>
  <%= f.text_field :house %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit "Update Character" %>


<%end%>
<%= link_to 'Delete!', house_character_path(@character), method: :delete, class: 'delete' %>
